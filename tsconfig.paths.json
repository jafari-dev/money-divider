{
  "compilerOptions": {
    "paths": {
      "#": ["./src/lib"],
      "#/*": ["./src/lib/*"],
      "@stores": ["./src/stores"],
      "@stores/*": ["./src/stores/*"],
      "_": ["./src/assets"],
      "_/*": ["./src/assets/*"],
      "@components": ["./src/components"],
      "@components/*": ["./src/components/*"],
      "@layouts": ["./src/layouts"],
      "@layouts/*": ["./src/layouts/*"],
      "@pages": ["./src/pages"],
      "@pages/*": ["./src/pages/*"],
    }
  }
}
